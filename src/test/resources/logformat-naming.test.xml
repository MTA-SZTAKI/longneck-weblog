<?xml version="1.0" encoding="UTF-8"?>
<log-format xmlns="urn:parser" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="urn:parser ../../main/resources/META-INF/longneck/parser/config.xsd">

  <!-- Common Log Format (CLF) -->
  <log-config>%v %h %u %l %t \"%r\" %s %b \"%{Referer}i\" \"%{User-agent}i\" \"%{Cookie}i\" \"%{Cookie}n\"</log-config>

  <log-elements>

    <log-element>
      <type>%r</type>
      <name>myRequest</name>
    </log-element>
    <log-element>
      <type>%{Referer}i</type>
      <name>myrefererUrl</name>
    </log-element>

  </log-elements>


</log-format>
